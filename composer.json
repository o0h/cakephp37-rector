{
    "name": "o0h/cakephp-37-rector",
    "description": "Auto fixer for CakePHP Fixtures import declaration to migrate CakePHP 3.7.x.",
    "type": "library",
    "require": {
       "php": "^7.1",
        "rector/rector": "^0.3.37",
        "cakephp/utility": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "O0h\\CakePHP37Rector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "O0h\\RectorCakePHP37FixtureImport\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hideki Kinjyo",
            "email": "dev@o0h.in"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
